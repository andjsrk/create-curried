{
	"compilerOptions": {
		"declaration": true,
		"erasableSyntaxOnly": true,
		"exactOptionalPropertyTypes": true,
		"forceConsistentCasingInFileNames": true,
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noEmitOnError": true,
		"outDir": "dist",
		"strict": true,
		"target": "esnext"
	},
	"include": ["src"]
}
